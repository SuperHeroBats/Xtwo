<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android">
    <!--设置圆角-->
    <corners android:radius="10dp" />
    <!--设置按钮颜色-->
    <solid android:color="#2895fa" />
    <!--按钮大小-->
    <size
        android:height="45dp"
        android:width="200dp"/>
    <!--设置内边距-->
    <padding
        android:top="5dp"
        android:bottom="5dp"
        android:left="5dp"
        android:right="5dp" />
</shape>